{
  "name": "arkfbp",
  "displayName": "ArkFBP - coding boosted",
  "description": "arkfbp - make your coding life easier",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.37.0"
  },
  "publisher": "longguikeji",
  "repository": {
    "type": "git",
    "url": "https://github.com/longguikeji/arkfbp-vscode-extension.git"
  },
  "author": {
    "name": "Rock Lee",
    "email": "insfocus@gmail.com"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "keywords": [
    "arkfbp",
    "ark",
    "fbp"
  ],
  "activationEvents": [
    "onView:arkfbp.explorer.info",
    "onView:arkfbp.explorer.flow",
    "onView:arkfbp.explorer.flowOutline",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:yaml"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "arkfbp.graph.preview",
        "title": "arkfbp: 预览流程图"
      },
      {
        "command": "arkfbp.createFlow",
        "title": "arkfbp: 新建工作流"
      },
      {
        "command": "arkfbp.createFlowNode",
        "title": "arkfbp: 新建工作流节点"
      },
      {
        "command": "arkfbp.views.explorer.action.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "arkfbp.explorer.flow.action.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "arkfbp.explorer.flow.action.create",
        "title": "Create",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "arkfbp.explorer.flow.action.run",
        "title": "Run the Flow",
        "icon": {
          "light": "resources/light/start.png",
          "dark": "resources/dark/start.png"
        }
      },
      {
        "command": "arkfbp.explorer.flow.action.delete",
        "title": "Delete the Flow",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "arkfbp.explorer.flowOutline.action.createFlowNode",
        "title": "CreateFlowNode",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "arkfbp.explorer.flowOutline.action.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "arkfbp.explorer.flowOutline.action.deleteFlowNode",
        "title": "Delete",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "arkfbp.welcome",
        "title": "Arkfbp",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "arkfbp.build",
        "title": "编译",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "arkfbp.explorer.info.action.build",
        "title": "编译",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "arkfbp.package.run",
        "title": "运行",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "arkfbp.package.test",
        "title": "测试",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "arkfbp.package.deploy",
        "title": "部署",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ],
    "views": {
      "arkfbp": [
        {
          "id": "arkfbp.explorer.info",
          "name": "App",
          "group": "navigation@1"
        },
        {
          "id": "arkfbp.explorer.flow",
          "name": "Flows",
          "group": "navigation@2"
        },
        {
          "id": "arkfbp.explorer.flowOutline",
          "name": "Outline",
          "group": "navigation@3"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "arkfbp.explorer.flow.action.refresh",
          "when": "view == arkfbp.explorer.flow",
          "group": "navigation"
        },
        {
          "command": "arkfbp.explorer.flow.action.create",
          "when": "view == arkfbp.explorer.flow",
          "group": "navigation"
        },
        {
          "command": "arkfbp.explorer.info.action.build",
          "when": "view == arkfbp.explorer.info",
          "group": "navigation"
        },
        {
          "command": "arkfbp.explorer.flowOutline.action.createFlowNode",
          "when": "view == arkfbp.explorer.flowOutline",
          "group": "navigation"
        },
        {
          "command": "arkfbp.explorer.flowOutline.action.refresh",
          "when": "view == arkfbp.explorer.flowOutline",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "arkfbp.explorer.flow.action.run",
          "when": "view == arkfbp.explorer.flow && viewItem == 'flow'",
          "group": "inline@1"
        },
        {
          "command": "arkfbp.explorer.flow.action.delete",
          "when": "view == arkfbp.explorer.flow && viewItem == 'flow'",
          "group": "inline@2"
        },
        {
          "command": "arkfbp.explorer.flowOutline.action.deleteFlowNode",
          "when": "view == arkfbp.explorer.flowOutline",
          "group": "inline"
        }
      ],
      "touchBar": [
        {
          "command": "arkfbp.welcome",
          "group": "navigation@+1"
        },
        {
          "command": "arkfbp.build",
          "group": "navigation@+2"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "arkfbp",
          "title": "ArkFBP",
          "icon": "resources/dark/activitybar.svg"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/js-yaml": "^3.12.3",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.37.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "jsonc-parser": "^0.4.2",
    "mocha": "^7.0.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "terser-webpack-plugin": "2.2.1"
  },
  "dependencies": {
    "js-yaml": "^3.13.1",
    "rimraf": "^3.0.2",
    "run-in-terminal": "0.0.3"
  }
}
